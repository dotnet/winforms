  </PropertyGroup>
  <!-- Package names -->
  <PropertyGroup>
    <XUnitStaFactPackage>xunit.stafact</XUnitStaFactPackage>
  </PropertyGroup>
  <!-- Package versions -->
  <PropertyGroup>
    <XUnitStaFactPackageVersion>0.3.5</XUnitStaFactPackageVersion>
  </PropertyGroup>
</Project>
