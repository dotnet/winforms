    <MicrosoftWin32SystemEventsPackage>Microsoft.Win32.SystemEvents</MicrosoftWin32SystemEventsPackage>
    <SystemDrawingDesignPrimitivesPackage>System.Drawing.Design.Primitives</SystemDrawingDesignPrimitivesPackage>
    <SystemDrawingPrimitivesPackage>System.Drawing.Primitives</SystemDrawingPrimitivesPackage>
    <SystemMediaSoundPlayerPackage>System.Media.SoundPlayer</SystemMediaSoundPlayerPackage>
    <SystemSecurityCryptographyCngPackage>System.Security.Cryptography.Cng</SystemSecurityCryptographyCngPackage>
    <SystemSecurityPermissionsPackage>System.Security.Permissions</SystemSecurityPermissionsPackage>
    <SystemCodeDomPackage>System.CodeDom</SystemCodeDomPackage>    
    <XUnitStaFactPackage>xunit.stafact</XUnitStaFactPackage>
    <MoqPackage>Moq</MoqPackage>
    <MicrosoftDotNetGenFacadesPackage>Microsoft.DotNet.GenFacades</MicrosoftDotNetGenFacadesPackage>
    <MicrosoftTargetingPackNETFrameworkv472Package>Microsoft.TargetingPack.NETFramework.v4.7.2</MicrosoftTargetingPackNETFrameworkv472Package>
