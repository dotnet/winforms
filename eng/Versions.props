  </PropertyGroup>
  <!-- Package names -->
  <PropertyGroup>
    <AccessibilityPackage>Accessibility</AccessibilityPackage>
    <MicrosoftWin32RegistryPackage>Microsoft.Win32.Registry</MicrosoftWin32RegistryPackage>
    <SystemConfigurationConfigurationManagerPackage>System.Configuration.ConfigurationManager</SystemConfigurationConfigurationManagerPackage>
