                    Guid guid =(Guid)UnsafeNativeMethods.PtrToStructure(val, typeof(Guid));
                    return guid;

                case (int)tagVT.VT_FILETIME:
                    longVal = ((int)data1 & 0xffffffff) | ((int)data2 << 32);
                    return new DateTime(longVal);

